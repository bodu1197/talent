{
  "threshold": 0,
  "reporters": ["html", "json", "console"],
  "ignore": [
    "**/__tests__/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/node_modules/**",
    "**/.next/**",
    "**/dist/**",
    "**/coverage/**",
    "**/migrations/**"
  ],
  "format": ["typescript", "javascript", "tsx", "jsx"],
  "minLines": 5,
  "minTokens": 50,
  "output": "./report"
}
